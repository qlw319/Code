package project.mynotepad.common_type;

public class BillTypes
{
	public static String strName;
	public static String strPicture;
	public static int nBillTypeID;

	public BillTypes()
	{
		strName = new String();
		strPicture = new String();
		nBillTypeID = 0;
	}

	public void Instead(BillTypes other)
	{
		if(strName != other.strName)
			strName = other.strName;
		if(strPicture != other.strPicture)
			strPicture = other.strPicture;
		nBillTypeID = other.nBillTypeID;
	}
}
