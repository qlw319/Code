package project.mynotepad.common_type;

public class WalletTypes
{
	public static String strName;
	public static String strType;
	public static int nColorIndex;

	public WalletTypes()
	{
		strName = new String();
		strType = new String();
		nColorIndex = -1;
	}
	
	public void Instead(WalletTypes other)
	{
		if(strName != other.strName)
			strName = other.strName;
		if(strType != other.strType)
			strType = other.strType;
		if(strColor != other.strColor)
			strColor = other.strColor;
	}
}
